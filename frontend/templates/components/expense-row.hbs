<tr key="{{id}}">
  {{!-- Hay que ver bien esto de #view-expense?id={{id}--}}

  <td><a href="#view-expense?id={{id}}">{{expense_type}}</a></td>
  <td>{{expense_date}}</td>
  <td>{{expense_quantity}}</td>
  <td>{{expense_description}}</td>
  <td>{{expense_file}}</td>
  <td>
    {{#if_eq user.currentUser expense_owner}}
      <button class="remove-button" item="{{id}}">{{i18n 'remove'}}</button>
      <button class="edit-button" item="{{id}}">{{i18n 'edit'}}</button>
    {{/if_eq}}
  </td>
</tr>

